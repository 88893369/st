			<linearGradient id="SVGID_4_" gradientUnits="userSpaceOnUse" x1="400.4336" y1="-718.7258" x2="391.1339" y2="-706.4739" gradientTransform="matrix(0.7071 0.7071 -0.7071 0.7071 -773.4638 250.0774)">
			<stop  offset="0" style="stop-color:#E94B30"/>
