  body .premium-read-more.st-bulleted-with-ad .paywall-box-area .paywallad-area img, body .premium-read-more.st-paragraph-with-ad .paywall-box-area .paywallad-area img { height: 100%; width: auto; }
  body .premium-read-more.st-bulleted-with-ad .paywall-box-area .paywallad-area img.padsimgmobile, body .premium-read-more.st-paragraph-with-ad .paywall-box-area .paywallad-area img.padsimgmobile { display: none; }
  body .premium-read-more.st-bulleted-with-ad .paywall-box-area .paywallad-area img.padsimg, body .premium-read-more.st-paragraph-with-ad .paywall-box-area .paywallad-area img.padsimg { display: block; height: 180px; }
